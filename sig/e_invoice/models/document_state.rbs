module EInvoice
  module Models
    type document_state = :DRAFT | :TRANSIT | :FAILED | :SENT | :RECEIVED

    module DocumentState
      extend EInvoice::Internal::Type::Enum

      DRAFT: :DRAFT
      TRANSIT: :TRANSIT
      FAILED: :FAILED
      SENT: :SENT
      RECEIVED: :RECEIVED

      def self?.values: -> ::Array[EInvoice::Models::document_state]
    end
  end
end
